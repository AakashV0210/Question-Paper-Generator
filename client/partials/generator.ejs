<div id="generator" style="display: none">
  <% if(path.includes("generate-question-view")){ %>
  <div id="question_generator_syllabus">

    SELECT SYLLABUS<br><br>
    <input type="radio" id="generator_ugc2015" name="generator_syllabus" onclick="display_generate_semester_form()"value="ugc 2015"></input>
    <label for="generator_ugc2015">UGC 2015</label><br>
    <input type="radio" id="generator_ugc2018" name="generator_syllabus" onclick="display_generate_semester_form()" value="ugc 2018"></input>
    <label for="generator_ugc2018">UGC 2018</label><br>
  </div>

    <br>
  <div id="question_generator_semester" style="display: none;">
    SELECT SEMESTER<br><br>
    <form action="#" id="generator_form"  onsubmit="return validate_question_generate()">
      <input type="radio" id="generator_sem_1" name="generator_semester"  value = "1"></input>
      <label for="generator_sem_1">1</label><br>
      <input type="radio" id="generator_sem_2" name="generator_semester"  value = "2"></input>
      <label for="generator_sem_2">2</label><br>
      <input type="radio" id="generator_sem_3" name="generator_semester"  value = "3"></input>
      <label for="generator_sem_3">3</label><br>
      <input type="radio" id="generator_sem_4" name="generator_semester" value = "4"></input>
      <label for="generator_sem_4">4</label><br>
      <input type="radio" id="generator_sem_5" name="generator_semester" value = "5"></input>
      <label for="generator_sem_5">5</label><br>
      <input type="radio" id="generator_sem_6" name="generator_semester" value = "6"></input>
      <label for="generator_sem_6">6</label><br>
      <input type="submit" value="Submit">
    </form>
    
  </div>
  
  <!-- <div id="generate_button" style="display: none;">
    <a href="#" id="generate_question_paper">cgh </a>
  </div> -->

  <% }else if(path.includes("generate-questions")){%>
  <div id="generated_questions">
    <% if(locals.generator_mark_2_data){ %>
    <ul>
      2 MARK QUESTIONS<br>
      <% var i = 1; generator_mark_2_data.forEach(function(data){ %>
        
        <% if(data != undefined){ %>
          <li><%=i%>. <%= data %></li>
        <% } else{ %>
          <li><%=i%>. NO QUESTIONS AVAILABLE</li>
        <% } %>

      <% i++; }) %>
    </ul>
    <br>

    <ul>
      6 MARK QUESTIONS<br>
      <% var i = 1; generator_mark_6_data.forEach(function(data){ %>
        
        <% if(data != undefined){ %>
          <li><%=i%>. <%= data %></li>
        <% } else{ %>
          <li><%=i%>. NO QUESTIONS AVAILABLE</li>
        <% } %>

      <% i++; }) %>
    </ul>
    <br>

    <ul>
      10 MARK QUESTIONS <br>
      <% var i = 1; generator_mark_10_data.forEach(function(data){ %>
        
        <% if(data != undefined){ %>
          <li><%=i%>. <%= data %></li>
        <% } else{ %>
          <li><%=i%>. NO QUESTIONS AVAILABLE</li>
        <% } %>

      <% i++; }) %>
    </ul>
    <% } %>
  </div>

  <div>
    <a href="/teacher_page/generate-question-view">BACK</a>
  </div>
  <% } %>
  
</div>